#include <stdio.h>
int main() {
    int num;
    printf("Enter a number (0 to 255): ");
    scanf("%d", &num);
    if (num < 0 || num > 255) {
        printf("Invalid input. Number must be between 0 and 255.\n");
        return 1;
    }
    printf("Binary of %d is: ", num);
    for (int i = 7; i >= 0; i--) {
        int bit = (num >> i) & 1;
        printf("%d", bit);
    }
    printf("\n");
    return 0;
}
