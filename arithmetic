#include <stdio.h>
int main() {
    int a = 10;
    int b = 5;
    printf("Initial values: a = %d, b = %d\n", a, b);
    a = b;
    printf("After a = b: a = %d\n", a);
    a += 3;  
    printf("After a += 3: a = %d\n", a);
    a -= 2;  
    printf("After a -= 2: a = %d\n", a);
    a *= 4;  
    printf("After a *= 4: a = %d\n", a);
    a /= 2; 
    printf("After a /= 2: a = %d\n", a);
    a %= 3;  
    printf("After a %%= 3: a = %d\n", a);
    return 0;
}
