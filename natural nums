#include <stdio.h>
int main() {
    int n, i;
    // Ask user for the value of n
    printf("Enter the value of n: ");
    scanf("%d", &n);
    // Print numbers from 1 to n
    printf("Natural numbers from 1 to %d are:\n", n);
    for(i = 1; i <= n; i++) {
        printf("%d ", i);
    }
    printf("\n");  // New line after printing all numbers
    return 0;
}
